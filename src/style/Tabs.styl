.whl_tabs {
  box-sizing: border-box;
  position: relative;
  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol';
  font-size: 12px;
  height: 46px;
  padding: 8px 3px 4px 3px;
  background: #dee1e6;
  border-radius: 5px 5px 0 0;
  overflow: hidden;
}

.whl_tabs * {
  box-sizing: inherit;
  font: inherit;
}

.whl_tabs .whl_tabs_content {
  position: relative;
  width: 100%;
  height: 100%;
}

*{
  cursor: default;
}

.whl_tabs .whl_tab {
  position: absolute;
  left: 0;
  height: 36px;
  width: 240px;
  border: 0;
  margin: 0;
  z-index: 1;
  pointer-events: none;
}

.whl_tabs .whl_tab, .whl_tabs .whl_tab * {
  user-select: none;
  cursor: default;
}

.whl_tabs .whl_tab .whl_tab-dividers {
  position: absolute;
  top: 7px;
  bottom: 7px;
  left: var(--tab-content-margin);
  right: var(--tab-content-margin);
}
.whl_tabs .whl_tab .whl_tab-dividers,
.whl_tabs .whl_tab .whl_tab-dividers::before,
.whl_tabs .whl_tab .whl_tab-dividers::after {
  pointer-events: none;
}
.whl_tabs .whl_tab .whl_tab-dividers::before,
.whl_tabs .whl_tab .whl_tab-dividers::after {
  content: "";
  display: block;
  position: absolute;
  top: 0;
  bottom: 0;
  width: 1px;
  background: #a9adb0;
  opacity: 1;
  transition: opacity 0.2s ease;
}
.whl_tabs .whl_tab .whl_tab-dividers::before {
  left: 0;
}
.whl_tabs .whl_tab .whl_tab-dividers::after {
  right: 0;
}

.whl_tabs .whl_tab:first-child .whl_tab-dividers::before { //,.whl_tabs .whl_tab:last-child .whl_tab-dividers::after
  opacity: 0;
}

.whl_tabs .whl_tab .whl_tab-background {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
  pointer-events: none;
}
.whl_tabs .whl_tab .whl_tab-background > svg {
  width: 100%;
  height: 100%;
}
.whl_tabs .whl_tab .whl_tab-background > svg .whl_tab-geometry {
  fill: #f4f5f6;
}
.whl_tabs .whl_tab[active] {
  z-index: 5;
}
.whl_tabs .whl_tab[active] .whl_tab-background > svg .whl_tab-geometry {
  fill: #fff;
}
.whl_tabs .whl_tab:not([active]) .whl_tab-background {
  transition: opacity 0.2s ease;
  opacity: 0;
}
@media (hover: hover) {
  .whl_tabs .whl_tab:not([active]):hover {
    z-index: 2;
  }
  .whl_tabs .whl_tab:not([active]):hover .whl_tab-background {
    opacity: 1;
  }
}
.whl_tabs .whl_tab.whl_tab-was-just-added {
  top: 10px;
  animation: whl_tab-was-just-added 120ms forwards ease-in-out;
}
.whl_tabs .whl_tab .whl_tab-content {
  position: absolute;
  display: flex;
  top: 0;
  bottom: 0;
  left: var(--tab-content-margin);
  right: var(--tab-content-margin);
  padding: 9px 8px;
  border-top-left-radius: 8px;
  border-top-right-radius: 8px;
  overflow: hidden;
  pointer-events: all;
}
.whl_tabs .whl_tab[is-mini] .whl_tab-content {
  padding-left: 2px;
  padding-right: 2px;
}
.whl_tabs .whl_tab .whl_tab-favicon {
  position: relative;
  flex-shrink: 0;
  flex-grow: 0;
  height: 16px;
  width: 16px;
  background-size: 16px;
  margin-left: 4px;
}
.whl_tabs .whl_tab[is-small] .whl_tab-favicon {
  margin-left: 0;
}
.whl_tabs .whl_tab[is-mini]:not([active]) .whl_tab-favicon {
  margin-left: auto;
  margin-right: auto;
}
.whl_tabs .whl_tab[is-mini][active] .whl_tab-favicon {
  display: none;
}
.whl_tabs .whl_tab .whl_tab-title {
  flex: 1;
  vertical-align: top;
  overflow: hidden;
  white-space: nowrap;
  margin-left: 4px;
  color: #5f6368;
  -webkit-mask-image: linear-gradient(90deg, #000 0%, #000 calc(100% - 24px), transparent);
  mask-image: linear-gradient(90deg, #000 0%, #000 calc(100% - 24px), transparent);
}
.whl_tabs .whl_tab[is-small] .whl_tab-title {
  margin-left: 0;
}
.whl_tabs .whl_tab .whl_tab-favicon + .whl_tab-title,
.whl_tabs .whl_tab[is-small] .whl_tab-favicon + .whl_tab-title {
  margin-left: 8px;
}
.whl_tabs .whl_tab[is-smaller] .whl_tab-favicon + .whl_tab-title,
.whl_tabs .whl_tab[is-mini] .whl_tab-title {
  display: none;
}
.whl_tabs .whl_tab[active] .whl_tab-title {
  color: #45474a;
}
.whl_tabs .whl_tab .whl_tab-drag-handle {
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  left: 0;
  border-top-left-radius: 8px;
  border-top-right-radius: 8px;
}
.whl_tabs .whl_tab .whl_tab-close {
  flex-grow: 0;
  flex-shrink: 0;
  position: relative;
  width: 16px;
  height: 16px;
  border-radius: 50%;
  background-image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 8 8'><path stroke='rgba(0, 0, 0, .65)' stroke-linecap='square' stroke-width='1.5' d='M0 0 L8 8 M8 0 L0 8'></path></svg>");
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 8px 8px;
}
@media (hover: hover) {
  .whl_tabs .whl_tab .whl_tab-close:hover {
    background-color: #e8eaed;
  }
  .whl_tabs .whl_tab .whl_tab-close:hover:active {
    background-color: #dadce0;
  }
}
@media not all and (hover: hover) {
  .whl_tabs .whl_tab .whl_tab-close:active {
    background-color: #dadce0;
  }
}
@media (hover: hover) {
  .whl_tabs .whl_tab:not([active]) .whl_tab-close:not(:hover):not(:active) {
    opacity: 0.8;
  }
}
.whl_tabs .whl_tab[is-smaller] .whl_tab-close {
  margin-left: auto;
}
.whl_tabs .whl_tab[is-mini]:not([active]) .whl_tab-close {
  display: none;
}
.whl_tabs .whl_tab[is-mini][active] .whl_tab-close {
  margin-left: auto;
  margin-right: auto;
}
@-moz-keyframes whl_tab-was-just-added {
  to {
    top: 0;
  }
}
@-webkit-keyframes whl_tab-was-just-added {
  to {
    top: 0;
  }
}
@-o-keyframes whl_tab-was-just-added {
  to {
    top: 0;
  }
}
@keyframes whl_tab-was-just-added {
  to {
    top: 0;
  }
}
 .whl_tab.whl_tab-was-just-dragged {
  transition: transform 120ms ease-in-out;
}

